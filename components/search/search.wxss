image{
  vertical-align: middle;
}
.search-wrap{
  display: flex;
  justify-content:space-between;
  height:76rpx;
  padding:0 34rpx 0 28rpx;
  width: 100%;
  box-sizing: border-box;
  margin-bottom: 37rpx;
}

.search-wrap .main{
  display: flex;
  justify-content: space-between;
  align-items:center;
  width: 100%;
  height:100%;
  padding:0 15rpx 0 26rpx;
  box-sizing: border-box;
background: #F3F3F3;
opacity: 1;
border-radius: 14rpx;
}
.search-wrap .main input{
  flex:1;
  margin:0 15rpx 0 15rpx;
font-size: 30rpx;
font-weight: 400;
line-height:76rpx;
color: #000000;
}
.search-wrap .main .icon-search{
  width:41rpx;
  height:41rpx;
}
.search-wrap .main .icon-close{
  width:35rpx;
  height:35rpx;
}

