/* -s*/
.switchwrap{
  height: 120rpx;
  display: flex;
  justify-content: center;
  vertical-align: center;
}
.switch {
  display: flex;
  justify-content: space-between;
  width:660rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: #E2E2E2;
  opacity: 1;
  border-radius: 60rpx;
  margin: auto;
  position: relative;
  z-index: 1;
}

.switch .active{
background: #F5D242!important;
}
.switch .off{
  display: inline-block;
  width: 200rpx;
height: 70rpx;

background: transparent;
opacity: 1;
border-radius: 60rpx;
text-align: center;
z-index: 2;
font-size: 30rpx;
font-weight: 400;
line-height: 70rpx;
color: #000000;
}

